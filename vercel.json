{
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/index.html",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/((?!assets/|favicon\\.svg|favicon\\.ico|robots\\.txt|sw\\.js|og-image\\.png|placeholder\\.svg).*)",
      "destination": "/index.html"
    }
  ]
}
